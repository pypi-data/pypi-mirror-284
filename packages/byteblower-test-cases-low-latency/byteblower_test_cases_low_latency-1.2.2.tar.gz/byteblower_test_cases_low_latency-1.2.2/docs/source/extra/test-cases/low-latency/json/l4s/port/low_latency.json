{
    "$schema": "https://api.byteblower.com/test-framework/json/test-cases/low-latency/config-schema.json",
    "server": "byteblower-server.example.com.",
    "ports": [
        {
            "name": "NSI",
            "port_group": [
                "classic_nsi"
            ],
            "interface": "trunk-1-17",
            "ipv4": "10.10.10.10",
            "gateway": "10.10.10.254"
        },
        {
            "name": "CPE",
            "port_group": [
                "classic_cpe"
            ],
            "interface": "trunk-1-18",
            "ipv4": "10.10.10.11",
            "gateway": "10.10.10.254"
        }
    ],
    "flows": [
        {
            "name": "Downstream UDP Flow",
            "type": "frame_blasting",
            "source": {
                "port_group": [
                    "classic_nsi"
                ]
            },
            "destination": {
                "port_group": [
                    "classic_cpe"
                ]
            },
            "initial_time_to_wait": 15.0,
            "duration": 80,
            "bitrate": 90e6,
            "analysis": {
                "latency": true
            }
        },
        {
            "name": "Downstream Gaming Flow",
            "type": "gaming",
            "dscp": "0x2B",
            "source": {
                "port_group": [
                    "classic_nsi"
                ]
            },
            "destination": {
                "port_group": [
                    "classic_cpe"
                ]
            },
            "udp_src": 55579,
            "udp_dest": 50022,
            "frame_rate": 10e3,
            "initial_time_to_wait": 30.0,
            "duration": 50,
            "analysis": {
                "latency": true
            }
        },
        {
            "name": "Downstream Voice Flow",
            "type": "voice",
            "dscp": "0x2B",
            "source": {
                "port_group": [
                    "classic_nsi"
                ]
            },
            "destination": {
                "port_group": [
                    "classic_cpe"
                ]
            },
            "initial_time_to_wait": 30.0,
            "duration": 50
        },
        {
            "name": "Downstream Classic HTTP",
            "type": "http",
            "source": {
                "port_group": [
                    "classic_nsi"
                ]
            },
            "destination": {
                "port_group": [
                    "classic_cpe"
                ]
            },
            "maximum_bitrate": 10e6,
            "receive_window_scaling": 12,
            "duration": 50
        },
        {
            "name": "Downstream L4S HTTP",
            "type": "http",
            "source": {
                "port_group": [
                    "classic_nsi"
                ]
            },
            "destination": {
                "port_group": [
                    "classic_cpe"
                ]
            },
            "enable_l4s": true,
            "maximum_bitrate": 10e6,
            "initial_time_to_wait": 60.0,
            "receive_window_scaling": 12
        }
    ],
    "report": {
        "html": true,
        "json": true,
        "junit_xml": false
    },
    "enable_scouting_flows": true,
    "maximum_run_time": 30.0
}
