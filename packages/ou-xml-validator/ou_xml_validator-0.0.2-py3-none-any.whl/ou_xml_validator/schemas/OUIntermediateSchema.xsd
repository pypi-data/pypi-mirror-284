<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2010 rel. 3 sp1 (http://www.altova.com) by Peter Cox (The Open University) -->
<!--OU Structured Authoring Generic Schema for OpenLearn v1.10-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:mml="http://www.w3.org/1998/Math/MathML"
	elementFormDefault="qualified">
	<xsd:import namespace="http://www.w3.org/1998/Math/MathML"
		schemaLocation="mathml3/xsd/mathml3.xsd"/>
	<xsd:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml/xml.xsd"/>
	<xsd:element name="Item">
		<xsd:annotation>
			<xsd:documentation>Book or Block</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="meta" type="metaStructure" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="CourseCode" type="MixedContentStructure"/>
				<xsd:element name="CourseTitle" type="RestrictedTitleStructure"/>
				<xsd:element name="ItemID" type="MixedContentStructure"/>
				<xsd:element name="ItemTitle" type="RestrictedTitleStructure"/>
				<xsd:element name="Notice" type="NoticeStructure" minOccurs="0"/>
				<xsd:element name="FrontMatter" type="FrontMatterStructure" minOccurs="0"/>
				<xsd:element name="Unit" type="UnitStructure" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="BackMatter" type="BackMatterStructure" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="TextType" type="TextTypeType" use="required"/>
			<xsd:attribute name="SchemaVersion" type="xsd:NMTOKEN" use="required" fixed="2.0"/>
			<xsd:attribute name="id" type="xsd:string"/>
			<xsd:attribute name="Template" type="ACSTemplateType"/>
			<xsd:attribute name="PageStartNumber" type="xsd:string"/>
			<xsd:attribute name="Rendering" type="LastRenderingType"/>
			<xsd:attribute name="AlternativeLanguage" type="LanguageType"/>
			<xsd:attribute ref="xml:lang"/>
			<xsd:attribute name="DiscussionAlias" type="DiscussionAliasType" default="Discussion"/>
			<xsd:attribute name="SecondColour" type="ColourType"/>
			<xsd:attribute name="ThirdColour" type="ColourType"/>
			<xsd:attribute name="FourthColour" type="ColourType"/>
			<xsd:attribute name="Logo" type="LogoType"/>
			<xsd:attribute name="vleglossary" type="VLEglossaryType" default="auto"/>
		</xsd:complexType>
		<xsd:unique name="UniqueId">
			<xsd:selector xpath=".//*"/>
			<xsd:field xpath="@id"/>
		</xsd:unique>
		<xsd:keyref name="KeyRefId" refer="UniqueId">
			<xsd:selector xpath=".//Paragraph/CrossRef | .//InlineStructure/CrossRef"/>
			<xsd:field xpath="@idref"/>
		</xsd:keyref>
		<xsd:unique name="FreeResponseId">
			<xsd:selector xpath=".//FreeResponse"/>
			<xsd:field xpath="@id"/>
		</xsd:unique>
		<xsd:keyref name="KeyRefFreeResponseId" refer="FreeResponseId">
			<xsd:selector xpath=".//FreeResponseDisplay"/>
			<xsd:field xpath="@idref"/>
		</xsd:keyref>
	</xsd:element>
	<xsd:simpleType name="TextTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CompleteItem"/>
			<xsd:enumeration value="FrontMatter"/>
			<xsd:enumeration value="BodyMatter"/>
			<xsd:enumeration value="BackMatter"/>
			<xsd:enumeration value="CourseGuide"/>
			<xsd:enumeration value="StudyGuide"/>
			<xsd:enumeration value="Introduction"/>
			<xsd:enumeration value="Essay"/>
			<xsd:enumeration value="Readings"/>
			<xsd:enumeration value="Acknowledgements"/>
			<xsd:enumeration value="References"/>
			<xsd:enumeration value="Glossary"/>
			<xsd:enumeration value="Index"/>
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DiscussionAliasType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value=""/>
			<xsd:enumeration value="Discussion"/>
			<xsd:enumeration value="Comment"/>
			<xsd:enumeration value="Feedback"/>
			<xsd:enumeration value="Solution"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ColourType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="None"/>
			<xsd:enumeration value="Black"/>
			<xsd:enumeration value="OU_Orange"/>
			<xsd:enumeration value="OU_Red"/>
			<xsd:enumeration value="OU_Pink"/>
			<xsd:enumeration value="OU_Violet"/>
			<xsd:enumeration value="OU_Blue"/>
			<xsd:enumeration value="OU_Turquoise"/>
			<xsd:enumeration value="OUBS_Green"/>
			<xsd:enumeration value="OUBS_Monochrome"/>
			<xsd:enumeration value="TECH_Blue"/>
			<xsd:enumeration value="TECH_DarkBlue"/>
			<xsd:enumeration value="TECH_Green"/>
			<xsd:enumeration value="Pantone2945C"/>
			<xsd:enumeration value="Pantone314C"/>
			<xsd:enumeration value="PantoneProcessBlueC"/>
			<xsd:enumeration value="Pantone326C"/>
			<xsd:enumeration value="Pantone356C"/>
			<xsd:enumeration value="Pantone377C"/>
			<xsd:enumeration value="Pantone272C"/>
			<xsd:enumeration value="Pantone2603C"/>
			<xsd:enumeration value="Pantone235C"/>
			<xsd:enumeration value="Pantone201C"/>
			<xsd:enumeration value="Pantone718C"/>
			<xsd:enumeration value="Pantone179C"/>
			<xsd:enumeration value="Pantone186C"/>
			<xsd:enumeration value="Pantone226C"/>
			<xsd:enumeration value="Pantone4525C"/>
			<xsd:enumeration value="Pantone7413C"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LogoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value=""/>
			<xsd:enumeration value="monochrome"/>
			<xsd:enumeration value="colour"/>
			<xsd:enumeration value="oubsmonochrome"/>
			<xsd:enumeration value="oubscolour"/>
			<xsd:enumeration value="oulawmonochrome"/>
			<xsd:enumeration value="oulawcolour"/>
			<xsd:enumeration value="ouwelshmonochrome"/>
			<xsd:enumeration value="ouwelshcolour"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VLEglossaryType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="auto"/>
			<xsd:enumeration value="manual"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LastRenderingType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Draft PDF"/>
			<xsd:enumeration value="Print PDF"/>
			<xsd:enumeration value="Print PDF + crop marks"/>
			<xsd:enumeration value="VLE2 modules (learn2)"/>
			<xsd:enumeration value="VLE2 shared (learn1)"/>
			<xsd:enumeration value="VLE2 staff (learn3)"/>
			<xsd:enumeration value="Qualification website (learn4)"/>
			<xsd:enumeration value="Open Science Lab (learn5)"/>
			<xsd:enumeration value="Prison VLE (learn7)"/>
			<xsd:enumeration value="XML"/>
			<xsd:enumeration value="OpenLearn"/>
			<xsd:enumeration value="OpenLearnCreate"/>
			<xsd:enumeration value="Accessible"/>
			<xsd:enumeration value="Alternative"/>
			<xsd:enumeration value="Audiobook"/>
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ACSTemplateListType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Generic_A4_Unnumbered"/>
			<xsd:enumeration value="Generic_Supp"/>
			<xsd:enumeration value="Wide_Margin_Reduced_A4"/>
			<xsd:enumeration value="Garamond_Crown_Quarto"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ACSTemplateTextType">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="ACSTemplateType">
		<xsd:union memberTypes="ACSTemplateListType ACSTemplateTextType"/>
	</xsd:simpleType>
	<xsd:complexType name="NoticeStructure">
		<xsd:sequence>
			<xsd:element name="Heading" type="HeadingStructure" minOccurs="0"/>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:element name="Box" type="BoxStructure"/>
				<xsd:element name="SubHeading" type="HeadingStructure"/>
				<xsd:element name="SubSubHeading" type="HeadingStructure"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attributeGroup ref="StructuralObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="FrontMatterStructure">
		<xsd:sequence>
			<xsd:element name="ByLine" type="ByLineStructure" minOccurs="0"/>
			<xsd:element name="HalfTitleVerso" type="HalfTitleVersoStructure" minOccurs="0"/>
			<xsd:element name="Imprint" type="ImprintStructure"/>
			<xsd:element name="TOC" type="TOCstructure" minOccurs="0"/>
			<xsd:element name="Preface" type="PrefaceStructure" minOccurs="0"/>
			<xsd:element name="Covers" type="CoversStructure" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UnitStructure">
		<xsd:annotation>
			<xsd:documentation>Chapter or Unit</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UnitID" type="MixedContentStructure"/>
			<xsd:element name="UnitTitle" type="TitleStructure"/>
			<xsd:element name="ByLine" type="ByLineStructure" minOccurs="0"/>
			<xsd:element name="Session" type="SessionStructure" maxOccurs="unbounded"/>
			<xsd:element name="BackMatter" type="BackMatterStructure" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:ID" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="BackMatterStructure">
		<xsd:sequence>
			<xsd:element name="Glossary" type="GlossaryStructure" minOccurs="0"/>
			<xsd:element name="Index" type="IndexStructure" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ByLineStructure" mixed="true">
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="IntroductionStructure">
		<xsd:sequence>
			<xsd:element name="Title" type="TitleStructure"/>
			<xsd:choice maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:element name="Section" type="SectionStructure"/>
				<xsd:group ref="LearningObjectsGroup"/>
				<xsd:group ref="MixedContentGroup"/>
			</xsd:choice>
			<xsd:element name="References" type="ReferencesStructure" minOccurs="0"/>
			<xsd:element name="FurtherReading" type="ReferencesStructure" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="StructuralObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="LearningOutcomesStructure">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="Paragraph" type="ParagraphStructure"/>
			<xsd:element name="LearningOutcome" type="ParagraphStructure"/>
		</xsd:choice>
		<xsd:attributeGroup ref="StructuralObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="SessionStructure">
		<xsd:sequence>
			<xsd:element name="Title" type="TitleStructure"/>
			<xsd:element name="Introduction" type="IntroductionStructure" minOccurs="0"/>
			<xsd:element name="LearningOutcomes" type="LearningOutcomesStructure" minOccurs="0"/>
			<xsd:choice maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:element name="Section" type="SectionStructure"/>
				<xsd:group ref="LearningObjectsGroup"/>
				<xsd:group ref="MixedContentGroup"/>
			</xsd:choice>
			<xsd:element name="Summary" type="SummaryStructure" minOccurs="0"/>
			<xsd:element name="References" type="ReferencesStructure" minOccurs="0"/>
			<xsd:element name="FurtherReading" type="ReferencesStructure" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="StructuralObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="TitleStructure" mixed="true">
		<xsd:sequence>
			<xsd:element name="Number" type="NumberStructure" minOccurs="0"/>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SideNote" type="SideNoteStructure"/>
				<xsd:element name="AuthorComment" type="InlineStructure"/>
				<xsd:element name="EditorComment" type="InlineStructure"/>
				<xsd:group ref="SentenceElementsGroup"/>
				<xsd:group ref="MixedContentGroup"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="RestrictedTitleStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="AuthorComment" type="InlineStructure"/>
			<xsd:element name="EditorComment" type="InlineStructure"/>
			<xsd:group ref="SentenceElementsGroup"/>
			<xsd:group ref="MixedContentGroup"/>
		</xsd:choice>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="HeadingStructure" mixed="true">
		<xsd:sequence>
			<xsd:element name="Number" type="NumberStructure" minOccurs="0"/>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SideNote" type="SideNoteStructure"/>
				<xsd:element name="AuthorComment" type="InlineStructure"/>
				<xsd:element name="EditorComment" type="InlineStructure"/>
				<xsd:group ref="SentenceElementsGroup"/>
				<xsd:group ref="MixedContentGroup"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute ref="xml:space" default="preserve"/>
		<xsd:attribute name="direction" default="" type="DirectionType"/>
	</xsd:complexType>
	<xsd:complexType name="TimingStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:sequence>
				<xsd:element name="Hours" type="xsd:nonNegativeInteger" minOccurs="0"/>
				<xsd:element name="Minutes" minOccurs="0">
					<xsd:simpleType>
						<xsd:restriction base="xsd:nonNegativeInteger">
							<xsd:maxInclusive value="59"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
			<xsd:group ref="SentenceElementsGroup"/>
			<xsd:group ref="MixedContentGroup"/>
			<xsd:element name="AuthorComment" type="InlineStructure" minOccurs="0"
				maxOccurs="unbounded"/>
			<xsd:element name="EditorComment" type="InlineStructure" minOccurs="0"
				maxOccurs="unbounded"/>
		</xsd:choice>
		<xsd:attributeGroup ref="LearningObjectsAttributesGroup"/>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="QADStructure">
		<xsd:choice maxOccurs="unbounded">
			<xsd:group ref="StructuralObjectsGroup"/>
			<xsd:group ref="SubHeadingsGroup"/>
			<xsd:group ref="LearningObjectsGroup"/>
			<xsd:group ref="MixedContentGroup"/>
		</xsd:choice>
		<xsd:attributeGroup ref="LearningObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="BoxStructure">
		<xsd:sequence>
			<xsd:element name="Heading" type="HeadingStructure" minOccurs="0"/>
			<xsd:choice maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:group ref="SubHeadingsGroup"/>
				<xsd:group ref="LearningObjectsGroup"/>
			</xsd:choice>
			<xsd:element name="SourceReference" type="SourceReferenceStructure" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="LearningObjectsAttributesGroup"/>
		<xsd:attribute name="type" type="BoxTypeType"/>
	</xsd:complexType>
	<xsd:complexType name="CaseStudyStructure">
		<xsd:sequence>
			<xsd:element name="Heading" type="HeadingStructure" minOccurs="0"/>
			<xsd:choice maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:group ref="SubHeadingsGroup"/>
				<xsd:group ref="LearningObjectsGroup"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attributeGroup ref="LearningObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="ExampleStructure">
		<xsd:sequence>
			<xsd:element name="Heading" type="HeadingStructure" minOccurs="0"/>
			<xsd:choice maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:group ref="SubHeadingsGroup"/>
				<xsd:element name="Answer" type="AnswerStructure"/>
				<xsd:group ref="LearningObjectsGroup"/>
			</xsd:choice>
			<xsd:element name="SourceReference" type="SourceReferenceStructure" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="LearningObjectsAttributesGroup"/>
	</xsd:complexType>
	<xsd:complexType name="ExtractStructure">
		<xsd:sequence>
			<xsd:element name="Heading" type="HeadingStructure" minOccurs="0"/>
			<xsd:element name="Introduction" type="IntroductionStructure" minOccurs="0"/>
			<xsd:element name="ByLine" type="ByLineStructure" minOccurs="0"/>
			<xsd:choice maxOccurs="unbounded">
				<xsd:group ref="StructuralObjectsGroup"/>
				<xsd:group ref="SubHeadingsGroup"/>
				<xsd:group ref="LearningObjectsGroup"/>
			</xsd:choice>
			<xsd:element name="SourceReference" type="SourceReferenceStructure" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="LearningObjectsAttributesGroup"/>
		<xsd:attribute name="class">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Journal"/>
					<xsd:enumeration value="Newspaper"/>
					<xsd:enumeration value="Web"/>
					<xsd:enumeration value="Fiction"/>
					<xsd:enumeration value="Other"/>
					<xsd:enumeration value=""/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SubsidiaryListItemStructure" mixed="true">
		<xsd:complexContent mixed="true">
			<xsd:restriction base="ListItemStructure">
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element name="SideNote" type="SideNoteStructure"/>
					<xsd:element name="Paragraph" type="ParagraphStructure"/>
					<xsd:element name="Equation" type="EquationStructure"/>
					<xsd:element name="Chemistry" type="ChemistryStructure"/>
					<xsd:element name="Figure" type="FigureStructure"/>
					<xsd:group ref="SentenceElementsGroup"/>
					<xsd:group ref="MixedContentGroup"/>
					<xsd:element name="ComputerDisplay" type="ComputerDisplayStructure"/>
					<xsd:element name="ProgramListing" type="ProgramListingStructure"/>
					<xsd:element name="AuthorComment" type="InlineStructure"/>
					<xsd:element name="EditorComment" type="InlineStructure"/>
					<xsd:element name="RevealMore" type="ParagraphWithoutSideNoteStructure"/>
				</xsd:choice>
				<xsd:attribute ref="xml:space" default="preserve"/>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ListItemStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="SideNote" type="SideNoteStructure"/>
			<xsd:element name="Paragraph" type="ParagraphStructure"/>
			<xsd:element name="Equation" type="EquationStructure"/>
			<xsd:element name="Chemistry" type="ChemistryStructure"/>
			<xsd:element name="Figure" type="FigureStructure"/>
			<xsd:group ref="SentenceElementsGroup"/>
			<xsd:group ref="MixedContentGroup"/>
			<xsd:element name="ComputerDisplay" type="ComputerDisplayStructure"/>
			<xsd:element name="ProgramListing" type="ProgramListingStructure"/>
			<xsd:element name="BulletedSubsidiaryList" type="SubsidiaryListStructure"/>
			<xsd:element name="NumberedSubsidiaryList" type="NumberedSubsidiaryListStructure"/>
			<xsd:element name="UnNumberedSubsidiaryList" type="SubsidiaryListStructure"/>
			<xsd:element name="AuthorComment" type="InlineStructure"/>
			<xsd:element name="EditorComment" type="InlineStructure"/>
			<xsd:element name="RevealMore" type="ParagraphWithoutSideNoteStructure"/>
		</xsd:choice>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="ListStructure">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="ListItem" type="ListItemStructure" maxOccurs="unbounded"/>
		</xsd:choice>
		<xsd:attribute name="direction" default="" type="DirectionType"/>
	</xsd:complexType>
	<xsd:complexType name="SubsidiaryListStructure">
		<xsd:sequence>
			<xsd:element name="SubListItem" type="SubsidiaryListItemStructure" maxOccurs="unbounded"
			/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NumberedListStructure">
		<xsd:complexContent>
			<xsd:extension base="ListStructure">
				<xsd:attribute name="class" type="NumberedListClassType" use="optional"
					default="decimal"/>
				<xsd:attribute name="start" type="xsd:positiveInteger" use="optional"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="NumberedSubsidiaryListStructure">
		<xsd:complexContent>
			<xsd:extension base="SubsidiaryListStructure">
				<xsd:attribute name="class" type="NumberedListClassType" use="optional"
					default="decimal"/>
				<xsd:attribute name="start" type="xsd:positiveInteger" use="optional"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParagraphStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="SideNote" type="SideNoteStructure"/>
			<xsd:element name="AuthorComment" type="InlineStructure"/>
			<xsd:element name="EditorComment" type="InlineStructure"/>
			<xsd:group ref="SentenceElementsGroup"/>
			<xsd:group ref="MixedContentGroup"/>
			<xsd:element name="SecondVoice" type="InlineStructure" minOccurs="0"
				maxOccurs="unbounded"/>
		</xsd:choice>
		<xsd:attribute ref="xml:space" default="preserve"/>
		<xsd:attribute name="direction" default="" type="DirectionType"/>
	</xsd:complexType>

	<xsd:complexType name="ParagraphWithoutSideNoteStructure" mixed="true">
		<xsd:complexContent>
			<xsd:restriction base="ParagraphStructure">
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element name="AuthorComment" type="InlineStructure"/>
					<xsd:element name="EditorComment" type="InlineStructure"/>
					<xsd:group ref="SentenceElementsGroup"/>
					<xsd:group ref="MixedContentGroup"/>
					<xsd:element name="SecondVoice" type="InlineStructure" minOccurs="0"
						maxOccurs="unbounded"/>
				</xsd:choice>
				<xsd:attribute ref="xml:space" default="preserve"/>
			</xsd:restriction>
		</xsd:complexContent>

	</xsd:complexType>

	<xsd:complexType name="SideNoteParagraphStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="AuthorComment" type="InlineStructure"/>
			<xsd:element name="EditorComment" type="InlineStructure"/>
			<xsd:group ref="SentenceElementsGroup"/>
			<xsd:group ref="MixedContentGroup"/>
		</xsd:choice>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="SideNoteStructure">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="Heading" type="RestrictedTitleStructure"/>
			<xsd:element name="SideNoteParagraph" type="SideNoteParagraphStructure"/>
			<xsd:group ref="MixedContentGroup"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="IconStructure" mixed="true">
		<xsd:attribute name="resource1" type="ResourceAttributeType" default="none"/>
		<xsd:attribute name="resource2" type="ResourceAttributeType" default="none"/>
		<xsd:attribute name="resource3" type="ResourceAttributeType" default="none"/>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="ComputerCodeStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="AuthorComment" type="InlineStructure"/>
			<xsd:element name="EditorComment" type="InlineStructure"/>
			<xsd:group ref="SentenceElementsWithoutComputerCodeAndComputerUIGroup"/>
			<xsd:group ref="MixedContentGroup"/>
		</xsd:choice>
		<xsd:attribute ref="xml:space" default="preserve"/>
	</xsd:complexType>
	<xsd:complexType name="ComputerUIStructure" mixed="true">
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="AuthorComment" type="InlineStructure"/>
			<xsd:element name="EditorComment" type="InlineStructure"/>
