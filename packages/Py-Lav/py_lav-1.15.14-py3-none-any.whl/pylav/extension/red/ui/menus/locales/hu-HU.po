#: pylav/extension/red/ui/menus/generic.py:123
#: pylav/extension/red/ui/menus/nodes.py:199
#: pylav/extension/red/ui/menus/playlist.py:260
#: pylav/extension/red/ui/menus/playlist.py:479
msgid "You are not authorized to interact with this."
msgstr "Ön nem jogosult arra, hogy ezzel interakcióba lépjen."

#: pylav/extension/red/ui/menus/nodes.py:60
msgid "Enter the domain or IP address of the host."
msgstr "Adja meg a gazdagép tartományát vagy IP-címét."

#: pylav/extension/red/ui/menus/nodes.py:61
#: pylav/extension/red/ui/menus/nodes.py:517
msgid "Host"
msgstr "Házigazda"

#: pylav/extension/red/ui/menus/nodes.py:68
msgid "Enter the host port to connect to."
msgstr "Adja meg a gazdaportot, amelyhez csatlakozni szeretne."

#: pylav/extension/red/ui/menus/nodes.py:69
#: pylav/extension/red/ui/menus/nodes.py:525
msgid "Port"
msgstr "Kikötő"

#: pylav/extension/red/ui/menus/nodes.py:76
msgid "Enter the node password."
msgstr "Adja meg a csomópont jelszavát."

#: pylav/extension/red/ui/menus/nodes.py:77
#: pylav/extension/red/ui/menus/nodes.py:533
msgid "Password"
msgstr "Jelszó"

#: pylav/extension/red/ui/menus/nodes.py:84
msgid "Enter an easy to reference name for the node."
msgstr "Adjon meg egy könnyen hivatkozható nevet a csomópontnak."

#: pylav/extension/red/ui/menus/nodes.py:85
#: pylav/extension/red/ui/menus/nodes.py:541
msgid "Name"
msgstr "Név"

#: pylav/extension/red/ui/menus/nodes.py:92
msgid "Enter a timeout in seconds."
msgstr "Adja meg az időtúllépést másodpercben."

#: pylav/extension/red/ui/menus/nodes.py:93
#: pylav/extension/red/ui/menus/nodes.py:549
msgid "Timeout"
msgstr "Időkorlát"

#: pylav/extension/red/ui/menus/nodes.py:109
msgid "Sources to turn off."
msgstr "A kikapcsolandó források."

#: pylav/extension/red/ui/menus/nodes.py:222
#: pylav/extension/red/ui/menus/nodes.py:689
msgid "The node name has been set to `{node_name_variable_do_not_translate}`."
msgstr "A csomópont neve '{node_name_variable_do_not_translate}`."

#: pylav/extension/red/ui/menus/nodes.py:237
#: pylav/extension/red/ui/menus/nodes.py:704
msgid "The node password has been set to `{node_password_variable_do_not_translate}`."
msgstr "A csomópont jelszava '{node_password_variable_do_not_translate}`."

#: pylav/extension/red/ui/menus/nodes.py:257
#: pylav/extension/red/ui/menus/nodes.py:724
msgid "The node host address has been set to `{node_host_variable_do_not_translate}`."
msgstr "A csomópont állomásának címe '{node_host_variable_do_not_translate}`."

#: pylav/extension/red/ui/menus/nodes.py:276
#: pylav/extension/red/ui/menus/nodes.py:743
msgid "The port you specified is invalid."
msgstr "A megadott port érvénytelen."

#: pylav/extension/red/ui/menus/nodes.py:284
#: pylav/extension/red/ui/menus/nodes.py:751
msgid "The node port has been set to `{node_port_variable_do_not_translate}`."
msgstr "A csomópont portja '{node_port_variable_do_not_translate}`."

#: pylav/extension/red/ui/menus/nodes.py:303
#: pylav/extension/red/ui/menus/nodes.py:770
msgid "The timeout you specified is invalid; it must be a number in seconds."
msgstr "A megadott időtúllépés érvénytelen; másodpercben megadott számnak kell lennie."

#: pylav/extension/red/ui/menus/nodes.py:311
#: pylav/extension/red/ui/menus/nodes.py:782
msgid "The node timeout has been set to `0` seconds."
msgstr "A csomópont időtúllépése \"0\" másodpercre van állítva."

#: pylav/extension/red/ui/menus/nodes.py:313
#: pylav/extension/red/ui/menus/nodes.py:778
msgid "The node timeout has been set to `1` second."
msgstr "A csomópont időtúllépése \"1\" másodpercre van állítva."

#: pylav/extension/red/ui/menus/nodes.py:315
#: pylav/extension/red/ui/menus/nodes.py:784
msgid "The node timeout has been set to `{node_timeout_variable_do_not_translate}` seconds."
msgstr "A csomópont időtúllépése a következőre van állítva: '{node_timeout_variable_do_not_translate}' másodperc."

#: pylav/extension/red/ui/menus/nodes.py:516
msgid "Change the domain or IP address of the host."
msgstr "Módosítsa az állomás tartományát vagy IP-címét."

#: pylav/extension/red/ui/menus/nodes.py:524
msgid "Change the host port to connect to."
msgstr "Módosítsa a gazdaportot a csatlakozáshoz."

#: pylav/extension/red/ui/menus/nodes.py:532
msgid "Change the node password."
msgstr "Módosítsa a csomópont jelszavát."

#: pylav/extension/red/ui/menus/nodes.py:540
msgid "Change the name of this node."
msgstr "Módosítsa a csomópont nevét."

#: pylav/extension/red/ui/menus/nodes.py:548
msgid "Enter the new timeout for this node."
msgstr "Adja meg a csomópont új időtúllépését."

#: pylav/extension/red/ui/menus/nodes.py:578
msgid "Source to disable."
msgstr "Forrás letiltása."

#: pylav/extension/red/ui/menus/player.py:94
msgid "selected"
msgstr "kiválasztott"

#: pylav/extension/red/ui/menus/player.py:104
msgid "inactive"
msgstr "inaktív"

#: pylav/extension/red/ui/menus/player.py:114
msgid "all"
msgstr "összes"

#: pylav/extension/red/ui/menus/player.py:207
msgid "I am not in any voice channel currently."
msgstr "Jelenleg nem vagyok egyetlen hangcsatornában sem."

#: pylav/extension/red/ui/menus/playlist.py:186
msgid "Please enter the playlist URL."
msgstr "Kérjük, adja meg a lejátszási lista URL-jét."

#: pylav/extension/red/ui/menus/playlist.py:187
#: pylav/extension/red/ui/menus/playlist.py:322
msgid "Playlist URL"
msgstr "Lejátszási lista URL-je"

#: pylav/extension/red/ui/menus/playlist.py:193
msgid "Please enter the playlist name."
msgstr "Kérjük, adja meg a lejátszási lista nevét."

#: pylav/extension/red/ui/menus/playlist.py:194
#: pylav/extension/red/ui/menus/playlist.py:329
msgid "Playlist Name"
msgstr "Lejátszási lista neve"

#: pylav/extension/red/ui/menus/playlist.py:321
msgid "Please enter the new URL for this playlist."
msgstr "Kérjük, adja meg a lejátszási lista új URL-jét."

#: pylav/extension/red/ui/menus/playlist.py:328
msgid "Please enter the new name for this playlist."
msgstr "Kérjük, adja meg a lejátszási lista új nevét."

#: pylav/extension/red/ui/menus/playlist.py:335
msgid "What is the query to add to this playlist?"
msgstr "Mi az a lekérdezés, amelyet hozzá kell adni ehhez a lejátszási listához?"

#: pylav/extension/red/ui/menus/playlist.py:336
#: pylav/extension/red/ui/menus/playlist.py:344
msgid "Query"
msgstr "Lekérdezés"

#: pylav/extension/red/ui/menus/playlist.py:343
msgid "What is the query to remove from this playlist?"
msgstr "Mi az a lekérdezés, amelyet el kell távolítani erről a listáról?"

#: pylav/extension/red/ui/menus/queue.py:457
msgid "Select a track to remove."
msgstr "Válassza ki az eltávolítani kívánt sávot."

#: pylav/extension/red/ui/menus/queue.py:459
msgid "Select a track to play now."
msgstr "Válaszd ki a most lejátszani kívánt zeneszámot."

