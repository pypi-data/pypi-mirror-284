#: pylav/extension/red/ui/menus/playlist.py:322
msgid "Playlist URL"
msgstr "URL playlistu"

#: pylav/extension/red/ui/menus/playlist.py:193
msgid "Please enter the playlist name."
msgstr "Zadejte název seznamu skladeb."

#: pylav/extension/red/ui/menus/playlist.py:194
#: pylav/extension/red/ui/menus/playlist.py:329
msgid "Playlist Name"
msgstr "Název playlistu"

